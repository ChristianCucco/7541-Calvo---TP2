
REDTELEFONICA.H

celular* buscarCelularPorNumero(int numero);

REDTELEFONICA.CPP

celular* redTelefonica::buscarCelularPorNumero(int numero)
{
    this->celularesExistentes->iniciarCursor();
   bool encontrado;
   while ((this->celularesExistentes->avanzarCursor())&&(!encontrado))
    {
        celularActual=celularesExistentes->obtenerCursor();
        encontrado=celularActual->numeroDeCelular()==numero;}
    return celularActual;
};

